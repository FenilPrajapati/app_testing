<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.module.category" id="module_category_freightbox">
        <field name="name">Freight Box</field>
        <field name="sequence">50</field>
        <field name="visible">true</field>
    </record>

    <record model="ir.module.category" id="module_category_operations">
        <field name="parent_id" ref="module_category_freightbox" />
        <field name="name">Operations</field>
        <field name="visible" eval="1" />
    </record>

    <record model="res.groups" id="group_operations">
        <field name="name">Administrator</field>
        <field name="category_id" ref="freightbox.module_category_operations"/>
    </record>

    <record model="ir.module.category" id="module_category_documentation">
        <field name="parent_id" ref="module_category_freightbox" />
        <field name="name">Documentation</field>
        <field name="visible" eval="1" />
    </record>

    <record model="res.groups" id="group_documentation">
        <field name="name">Administrator</field>
        <field name="category_id" ref="freightbox.module_category_documentation"/>
    </record>

</odoo>
