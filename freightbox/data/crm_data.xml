<?xml version="1.0" encoding="utf-8"?>
<odoo>

    

    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'crm'), ('name', 'in', ['stage_lead1','stage_lead2','stage_lead3','stage_lead4'])]"/>
        </function>
        <value eval="{'noupdate': False}" />
    </function>

    <record id="crm.stage_lead1" model="crm.stage">
        <field name="state">new</field>
    </record>
    <record id="crm.stage_lead2" model="crm.stage">
        <field name="state">qualified</field>
    </record>
    <record id="crm.stage_lead3" model="crm.stage">
        <field name="state">proposition</field>
    </record>
    <record id="crm.stage_lead4" model="crm.stage">
        <field name="state">won</field>
    </record>

    <function name="write" model="ir.model.data">
        <function name="search" model="ir.model.data">
            <value eval="[('module', '=', 'crm'), ('name', 'in', ['stage_lead1','stage_lead2','stage_lead3','stage_lead4'])]"/>
        </function>
        <value eval="{'noupdate': True}" />
    </function>

</odoo>
