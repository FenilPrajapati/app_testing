<?xml version='1.0' encoding='utf-8'?>
<odoo noupdate="1">

    <record forcecreate="True" id="auto_currency_rate_updates" model="ir.cron">
        <field name="name">Automatic currency rates update</field>
        <field name="interval_number">8</field>
        <field name="interval_type">hours</field>
        <field name="numbercall">-1</field>
        <field name="doall">0</field>
        <field name="active">1</field>
        <field name="model_id" ref="model_currency_rate_provider"/>
        <field name="state">code</field>
        <field name="code">model._schedule_auto_currency_rate()</field>
    </record>

