<?xml version="1.0" encoding="utf-8"?>
<template id="SQDashboard">

    <t t-name="freightbox.SQDashboard">
        <div class="o_sq_dashboard container">
            <div class="row">
                <div class="col-sm-7">
                    <table class="table table-sm">
                        <tbody>
                            <tr>
                                <td class="o_text">
                                    <div>All Events</div>
                                </td>
                                <td class="o_main o_sq_dashboard_action" title="Draft" name="freightbox.action_shipment_quote" context='{"search_default_sq_draft": true}'>
                                    <a href="#" style="color: white;"><t t-esc="values['draft']"/><br/>Draft</a>
                                </td>
                                <td class="o_main o_sq_dashboard_action" title="Accepted" name="freightbox.action_shipment_quote" context='{"search_default_sq_accepted": true}'>
                                    <a href="#" style="color: white;"><t t-esc="values['accepted']"/><br/>Accepted</a>
                                </td>
                                <td class="o_main o_sq_dashboard_action" title="Rejected" name="freightbox.action_shipment_quote" context='{"search_default_sq_rejected": true}'>
                                    <a href="#" style="color: white;"><t t-esc="values['rejected']"/><br/>Rejected</a>
                                </td>
                                <td class="o_main o_sq_dashboard_action" id="sq_cancel" title="Cancelled" name="freightbox.action_shipment_quote" context='{"search_default_sq_cancelled": true}'>
                                    <a href="#" style="color: white;"><t t-esc="values['cancelled']"/><br/>Cancelled</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </t>
        
</template>
