                        <span class="arrow_freight_bc">&#8672;</span>
                        <div class="col2">
                            <!-- <div class="rectangle_bc freight_hold_bol" style="background:#5B2C6F">
                                  <h3 class="circle_count_bc"><t t-esc="widget.hold_bol_count"/></h3>
                                <h2 class="freight_text_bc">Hold BOL</h2>
                            </div> -->
                            <div class="rectangle_bc freight_switch_bol" style="background:#9B9B19">
                                  <h3 class="circle_count_bc"><t t-esc="widget.switch_bol_count"/></h3>
                                <h2 class="freight_text_bc">Switch BOL</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8672;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_amend_bol" style="background:#502C2C">
                                  <h3 class="circle_count_bc"><t t-esc="widget.amend_bol_count"/></h3>
                                <h2 class="freight_text_bc">Amend BOL</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8672;</span>
                        <div class="col2">
                            <!-- <div class="rectangle_bc freight_switch_bol" style="background:#9B9B19">
                                  <h3 class="circle_count_bc"><t t-esc="widget.switch_bol_count"/></h3>
                                <h2 class="freight_text_bc">Switch BOL</h2>
                            </div> -->
                            <div class="rectangle_bc freight_hold_bol" style="background:#5B2C6F">
                                  <h3 class="circle_count_bc"><t t-esc="widget.hold_bol_count"/></h3>
                                <h2 class="freight_text_bc">Hold BOL</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8592;</span>
                        <div class="col2">
                            <!-- <div class="rectangle_bc freight_surrender_bol" style="background:#6235cb">
                                  <h3 class="circle_count_bc"><t t-esc="widget.surrender_bol_count"/></h3>
                                <h2 class="freight_text_bc">Surrender BOL</h2>
                            </div> -->
                            <div class="rectangle_bc freight_issue_bol" style="background:#B18041">
                                  <h3 class="circle_count_bc"><t t-esc="widget.issue_bol_count"/></h3>
                                <h2 class="freight_text_bc">Issue BOL</h2>
                            </div>
                        </div>
                    </div>
                    <div class="row down_arrow_bc_left" >
                        <span>&#8595;</span>
                    </div>
                    <div class="row" style="height: 75px;">
                        <div class="col2">
                            <div class="rectangle_bc freight_release_cargo" style="background:#2C504A">
                                  <h3 class="circle_count_bc"><t t-esc="widget.release_cargo_count"/></h3>
                                <h2 class="freight_text_bc">Release Cargo</h2>
                            </div>
                        </div>
                        <!--<span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc" style="background:#E67E22">
                                  <h3 class="circle_count_bc">15</h3>
                                  <h2 class="freight_text_bc">Release Cargo</h2>
                            </div>
                        </div>-->
                    </div>
                </div>
                </section>
            </div>
        </div>
    </t>
