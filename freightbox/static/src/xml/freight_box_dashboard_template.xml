<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="FreightBoxDashboardMain">
        <div>
            <div class="wrap" style="overflow-y: scroll; height: 600px;">
                <style>
                    .circle_count_bc {
                        text-align: right;
                        font-size: large;
                        color: azure;
                        padding-right: 7px;
                    }

                    .rectangle_bc {
                        display: block;
                        height: 62px;
                        width: 157px;
                        background: #2c9956;
                        position: relative;
                        border-top-right-radius: 0em;
                        border-top-left-radius: 3em;
                        border-bottom-right-radius: 3em;
                        box-shadow: 5px 5px 5px gray;
                        cursor: pointer;
                    }

                    .freight_text_bc {
                            font-size: 14px;
                            text-align: center;
                            color: white;
                            margin-top: -6px;
                            font-style: italic;
                        }
                    .arrow_freight_bc {
                            font-size: 40px;
                            color: #401919;
                            padding-left: 7%;
                    }

                    .down_arrow_bc {
                        align-items: end;
                        font-size: 42px;
                        justify-content: right;
                        margin-right: 150px;
                    }
                    .down_arrow_bc_left {
                        align-items: end;
                        font-size: 42px;
                        justify-content: left;
                        margin-left: 50px;
                    }

                    .row > * {
                        flex-shrink: 0;
                        width: 9%;
                        max-width: 100%;
                        padding-right: calc(var(--gutter-x) * .5);
                        margin-top: var(--gutter-y);
                    }
                </style>
                <section>
                <div class="container" style="overflow-y: scroll; height: 600px;">
                    <div class="row" style="height: 75px;">
                    </div>
                    <div class="row" style="height: 75px;">
                        <div class="col2">
                            <div class="rectangle_bc freight_enquiry" style="background:#5A9125">
                                  <h3 class="circle_count_bc"><t t-esc="widget.crm_count"/></h3>
                                <h2 class="freight_text_bc">Inquiry</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_rc" style="background:#E67E22">
                                  <h3 class="circle_count_bc"><t t-esc="widget.rc_count"/></h3>
                                <h2 class="freight_text_bc">Rate Comparison</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <!--<div class="stat-icon"><i class="fa fa-money"/></div>-->
                        <div class="col2">
                            <div class="rectangle_bc freight_draft_po" style="background:#9B9B19">
                                  <h3 class="circle_count_bc"><t t-esc="widget.draft_po_count"/></h3>
                                <h2 class="freight_text_bc">Draft PO</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_sq" style="background:#B18041">
                                  <h3 class="circle_count_bc"><t t-esc="widget.sq_count"/></h3>
                                <h2 class="freight_text_bc">Shipment Quote</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_draft_so" style="background:#5B2C6F">
                                  <h3 class="circle_count_bc"><t t-esc="widget.draft_so_count"/></h3>
                                <h2 class="freight_text_bc">Draft SO</h2>
                            </div>
                        </div>
                    </div>
                    <div class="row down_arrow_bc" style="height:75px;">
                        <span>&#8595;</span>
                    </div>
                    <div class="row" style="height: 75px;">
                        <div colspan="2">
                            <div class="rectangle_bc freight_open_si" style="background:#5B2C6F">
                                  <h3 class="circle_count_bc"><t t-esc="widget.open_si_count"/></h3>
                                <h2 class="freight_text_bc">Open SI</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8592;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_transport" style="background:#502C2C">
                                  <h3 class="circle_count_bc"><t t-esc="widget.transport_count"/></h3>
                                <h2 class="freight_text_bc">Transport</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8592;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_job" style="background:#CB4335">
                                  <h3 class="circle_count_bc"><t t-esc="widget.job_count"/></h3>
                                <h2 class="freight_text_bc">Job</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8592;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_so" style="background:#4D2C50">
                                  <h3 class="circle_count_bc"><t t-esc="widget.so_count"/></h3>
                                <h2 class="freight_text_bc">Sale Order</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8592;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_po" style="background:#2C504A">
                                  <h3 class="circle_count_bc"><t t-esc="widget.po_count"/></h3>
                                <h2 class="freight_text_bc">Purchase Order</h2>
                            </div>
                        </div>
                    </div>
                    <div class="row down_arrow_bc_left" >
                        <span>&#8595;</span>
                    </div>
                    <div class="row" style="height: 75px;">
                        <div class="col2">
                            <div class="rectangle_bc freight_draft_si" style="background:#E67E22">
                                  <h3 class="circle_count_bc"><t t-esc="widget.draft_si_count"/></h3>
                                <h2 class="freight_text_bc">Draft SI</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_update_si" style="background:#0d9b00fb">
                                  <h3 class="circle_count_bc"><t t-esc="widget.update_si_count"/></h3>
                                <h2 class="freight_text_bc">Update SI</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_approved_si" style="background:#5F73D4">
                                  <h3 class="circle_count_bc"><t t-esc="widget.approve_si_count"/></h3>
                                <h2 class="freight_text_bc">Approved SI</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_draft_bol" style="background:#B1417C">
                                  <h3 class="circle_count_bc"><t t-esc="widget.draft_bol_count"/></h3>
                                <h2 class="freight_text_bc">Draft BOL</h2>
                            </div>
                        </div>
                        <span class="arrow_freight_bc">&#8594;</span>
                        <div class="col2">
                            <div class="rectangle_bc freight_update_bol" style="background:#7B7D7D">
                                  <h3 class="circle_count_bc"><t t-esc="widget.update_bol_count"/></h3>
                                <h2 class="freight_text_bc">Update BOL</h2>
                            </div>
                        </div>
                    </div>
                    <div class="row down_arrow_bc">
                        <span>&#8595;</span>
                    </div>
                    <div class="row" style="height: 75px;">
                        <div class="col2">
                            <div class="rectangle_bc freight_surrender_bol" style="background:#6235cb">
                                  <h3 class="circle_count_bc"><t t-esc="widget.surrender_bol_count"/></h3>
                                <h2 class="freight_text_bc">Surrender BOL</h2>
                            </div>
                            <!-- <div class="rectangle_bc freight_issue_bol" style="background:#B18041">
                                  <h3 class="circle_count_bc"><t t-esc="widget.issue_bol_count"/></h3>
                                <h2 class="freight_text_bc">Issue BOL</h2>
                            </div> -->
