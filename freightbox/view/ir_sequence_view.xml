<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>



        <record id="sequence_container_type" model="ir.sequence">
            <field name="name">Container Type</field>
            <field name="code">container.type.seq</field>
            <field name="prefix">CT</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"></field>
        </record>

        <record id="sequence_crm_lead" model="ir.sequence">
            <field name="name">CRM</field>
            <field name="code">crm.lead.seq</field>
            <field name="prefix">erpbox</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"></field>
        </record>

        <record id="sequence_job" model="ir.sequence">
            <field name="name">Job</field>
            <field name="code">job.seq</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"></field>
        </record>

        <record id="sequence_shipping_instruction" model="ir.sequence">
            <field name="name">Shipping Instruction</field>
            <field name="code">shipping.instruction.seq</field>
            <field name="prefix">SI</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"></field>
        </record>

    </data>
    <data>
        <record id="seq_id_shipping_instruction" model="ir.sequence">
            <field name="name">Shipping Instruction Model</field>
            <field name="code">shipping.instruction.si.seq</field>
            <field name="prefix">SI%(day)s%(month)s</field>
            <field eval="True" name="use_date_range"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_transport" model="ir.sequence">
            <field name="name">Transport</field>
            <field name="code">transport.seq</field>
            <field name="prefix">TID</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"></field>
        </record>

    </data>
</odoo>

