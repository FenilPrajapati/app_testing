        <record id="rate_charges_tree_view" model="ir.ui.view">
            <field name="name">Charges</field>
            <field name="model">charges</field>
            <field name="arch" type="xml">
                <tree string="Charges" create="false" delete="false" edit="false" editable="bottom" sample="1">

                    <header>
                        <button name="add_charges_rate_comparision" string="Select Charges" type="object"
