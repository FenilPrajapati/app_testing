                        <button name="cancel_charges" string="Cancel" type="object" class="btn-primary"/>
                    </header>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="currency_id"/>
                    <field name="type_of_charges"/>
                </tree>
            </field>
        </record>
        <!-- ********** End ******* -->


