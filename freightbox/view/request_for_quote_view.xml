        <!-- ********** RFQ form view ********** -->
        <record id="rfq_form_view" model="ir.ui.view">
            <field name="name">Rate Comparison</field>
            <field name="model">request.for.quote</field>
            <field name="inherit_id" ref="base_freightbox.rfq_form_view"/>
            <field name="arch" type="xml">
<!--                <form string="Rate Comparison">-->
                    <xpath expr="//button[@name='button_approve']" position="after">
                        <button name="%(freightbox.act_rc_reject_wiz)d" states="draft"
                                string="Reject" type="action" class="oe_highlight"
                                help="Rejects this Rate Comparision"/>
                         <button name="%(freightbox.act_rc_correct_wiz)d" string="Correct"
                                attrs="{'invisible': ['|', ('job_id', '!=', False), ('state', 'not in', ('approved', 'correction_done'))]}"
                                 type="action" class="oe_highlight"
                                help="Gives an option to make any corrections in this Rate Comparision"/>
                        <button name="button_confirm_correction" string="Confirm Correction"
                                states="under_correction" type="object" class="oe_highlight"
                                confirm="Are you sure you want to confirm Correct?"
                                help="Confirms any corrections that are made,in this Rate Comparision"/>
                    </xpath>
                    <xpath expr="//div[hasclass('oe_button_box')]" position="inside">
                        <button name="action_get_po"
                                    type="object" attrs="{'invisible': [('state', 'in', ['draft'])]}"
                                    class="oe_stat_button"
                                    icon="fa-pencil-square-o" string="Purchase Order"
                                    help="Shows the purchase order,related to this Rate Comparision">
                            </button>
                    </xpath>
                    <xpath expr="//field[@name='company_id']" position="after">
                        <field name="reject_bool" invisible="1"/>
                        <field name="correct_reason_bool" invisible="1"/>
                        <field name="amend_bool" invisible="1"/>
                        <field name="job_id" invisible="1"/>
                    </xpath>
                    <xpath expr="//page[@name='rate']" position="after">
                        <page name="reason" string="Reject Reason"
                                  attrs="{'invisible': [('reject_bool','!=', True)]}">
                                <group colspan="2" col="2">
                                    <field name="reject_reason" readonly="1"/>
                                    <field name="comment"/>
                                </group>
                            </page>
                            <page name="correction" string="Correction Reason"
                                  attrs="{'invisible': [('correct_reason_bool','!=', True)]}">
                                <group colspan="2" col="2">
                                    <field name="correct_reason" readonly="1"/>
                                </group>
                            </page>
                        <page string="Video Tutorial">
                                <button name="open_rfq_tutorial_video_new_tab" type="object" class="btn-primary" string="Open Full Tutorial Video in New Tab"/>
                                <div class="app_settings_block" string="Rate Comparison Tutorial">
                                    <div class="row mt16 o_settings_container" name="qualification_setting_container">
                                        <div controls="true"  class="embed-responsive embed-responsive-16by9">
                                            <iframe id="rfq_tutorial_video" width="1080px" height="720px" src=""></iframe>
                                        </div>
                                    </div>
                                </div>
                            </page>
                    </xpath>
                    <xpath expr="//page[@name='rate']" position="inside">
                        <group>
                                    <group>
                                        <field name="charges_template_ids" widget="many2many_tags"
                                               string='Charges Template'
                                               domain="[('charges_template_line','!=', False)]"
                                               attrs="{'readonly': [('state', 'in', ('approved', 'cancelled','expired','under_correction'))]}"/>
                                    </group>
                                </group>
                                <header>
                                    <button name="action_load_charges" type="object" string="Load Charges"
                                            states="draft,rejected" class="btn-primary"
                                            help="Loads the charges from the charges template selected above"/>
                                    <button name="delete_charges" type="object" string="Delete Charges"
                                            states="draft,rejected,under_correction" class="btn-primary"
                                            confirm="This will delete all the charges,do you wish to proceed?"
                                            help="Deletes all the charges below"/>
                                    <button name="action_charges" type="object" string="Select Charges"
                                            states="draft,under_correction,rejected" class="btn-primary"
                                            help="Selects multiple charges from the list of charges"/>
                                    <button name="action_create_charges_template" type="object"
                                            string="Create/Update Charges Template" class="btn-primary"
                                            help="Creates a charge template with the below listed charges"/>
                                    <!--<button string="Update Charges Prepaid/Collect" type="object"
                                            states="draft,rejected"
                                            name="action_update_charges_prepaid_collect" class="oe_highlight"/>-->
                                </header>
                    </xpath>
