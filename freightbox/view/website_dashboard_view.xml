    &amp;:nth-child(15){order: 12;margin-left: 225px;}


    &amp;:nth-child(4),&amp;:nth-child(5),&amp;:nth-child(12),&amp;:nth-child(13){
      .arrow_freight_right {
        display: none;
      }
    }

    &amp;:nth-child(8),&amp;:nth-child(9),&amp;:nth-child(10),&amp;:nth-child(15),&amp;:nth-child(14){
