     <template id="freight_box_dashboard" name="Dashboard">
        <t t-call="website.layout">
            <div id="wrap">
                <style>
                    
.row{
  display: flex;
  justify-content: center;
  gap: 60px 0px;
}



.circle {
  position: relative;
  height: 30px;
  width: 51px;
  border-radius: 14px;
  border: 3px solid white;
  left: 50%;
  margin-left: -25px;
  top: -20px;
  background: red;
}
.down_arrow_left {
    height: 77px;
    align-items: end;
    font-size: 42px;
    padding-left: 10px;
    margin-top: 10px;
}

.down_arrow {
    font-size: 45px;
    padding-right: 1px;
    margin-top: 12px;
}



.rectangle {
  display: block;
  height: 62px;
  width: 157px;
  background: #2c9956;
  position: relative;
  margin-top: 12px;
  border-top-right-radius: 0em;
  border-top-left-radius: 3em;
  border-bottom-right-radius: 3em;
  box-shadow: 5px 5px 5px gray;
  cursor: pointer;
}
.circle_count {
  text-align: right;
  font-size: large;
  color: azure;
  padding-right: 7px;
}
.freight_text {
  font-size: 14px;
  text-align: center;
  color: white;
  margin-top: -6px;
  font-style: italic;
}
.arrow_freight_right {
  // display: none;
  font-size: 40px;
  color: #401919;

}
.arrow_freight_left {
  display: none;
  transform: rotate(180deg);
  font-size: 40px;
  color: #401919;

}
.arrow_freight_down {
  display: none;
  transform: rotate(90deg);
  position: absolute;
  left: 40%;
  bottom: -100%;
  font-size: 40px;
  color: #401919;
}


.col2{
  position: relative;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  width: 225px;

  &amp;:nth-child(5n){

    .arrow_freight_right{
      display: none;
    }

  }

  &amp;:nth-child(6){

    .arrow_freight_down {
      display: block;
    }
  }

  &amp;:nth-child(5){

    .arrow_freight_down {
      display: block;
    }

  }

  &amp;:nth-child(6),&amp;:nth-child(7),&amp;:nth-child(8),&amp;:nth-child(9){
    .arrow_freight_right{
      display: none;
    }

    .arrow_freight_left {
      display: block;
    }
  }
}

@media (max-width: 1399.98px) {

}

// Large devices (desktops, less than 1200px)
@media (max-width: 1199.98px) {

  .col2{
    //reset the css
    &amp;:nth-child(n){
      .arrow_freight_right {
        display: block;
      }
      .arrow_freight_left {
        display: none;
      }
      .arrow_freight_down {
        display: none;
      }
    }


    //ordering
    &amp;:nth-child(1){order: 0;}
    &amp;:nth-child(2){order: 1;}
    &amp;:nth-child(3){order: 2;}
    &amp;:nth-child(4){order: 3;}
    &amp;:nth-child(5){order: 7;}
    &amp;:nth-child(6){order: 9;}
    &amp;:nth-child(7){order: 8;}
