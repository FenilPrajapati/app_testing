        <record id="shipment_quote_form_view" model="ir.ui.view">
            <field name="name">Shipment Quote</field>
            <field name="model">shipment.quote</field>
            <field name="inherit_id" ref="base_freightbox.shipment_quote_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//button[@name='button_set_to_draft']" position="after">
                    <button name="%(freightbox.act_sq_correct_wiz)d" attrs="{'invisible': ['|', ('job_id', '!=', False), ('state', 'not in', ('accepted', 'correction_done'))]}"
                                string="Correct" type="action" class="oe_highlight"/>
                        <button name="button_confirm_correction"
                                confirm="Are you sure, you want to confirm this correction?"
                                string="Confirm Correction" states="under_correction" type="object"
                                class="oe_highlight"/>
                </xpath>
                <xpath expr="//div[hasclass('oe_button_box')]" position="inside">
                    <button name="action_get_so"
                                    type="object" attrs="{'invisible': [('state', 'in', ['draft','rejected'])]}"
                                    class="oe_stat_button"
                                    icon="fa-pencil-square-o" string="Sale Order">
                            </button>
                </xpath>
                <xpath expr="//page[@name='rate']" position="inside">
                    <group name="sq_markup">
                                    <group>
                                        <field name="same_as_cp"/>
                                        <field name="job_id" readonly="1" invisible="1"/>
                                        <field name="correct_reason_bool" readonly="1" invisible="1"/>
                                        <field name="markup"
                                               attrs="{'readonly': ['|', ('state', 'not in', ('draft','under_correction')),('same_as_cp', '=', True)]}"/>
                                        <button name="update_sale_price" string="Update Sale Price"
                                                states="draft,under_correction" type="object" class="oe_highlight"/>

                                    </group>
                                </group>
                </xpath>
                <xpath expr="//page[@name='rate']" position="after">
                    <page name="reason" string="Reject Reason"
                                  attrs="{'invisible': [('reject_bool','!=', True)]}">
                                <!-- <group colspan="2" col="2"> -->
                                    <field name="sq_reject_reason">
                                        <tree string="Reject reason" editable="bottom">
                                            <field name="sq_id" invisible="1"/>
                                            <field name="name"/>
                                        </tree>
                                    </field>
                                <!-- </group> -->
                            </page>
                            <page name="correction" string="Correction Reason"
                                  attrs="{'invisible': [('correct_reason_bool','!=', True)]}">
                                <group colspan="2" col="2">
                                    <field name="correct_reason" readonly="1"/>
                                </group>
                            </page>
                            <page string="Video Tutorial">
                                <button name="open_shipment_quote_tutorial_video_new_tab" type="object" class="btn-primary" string="Open Full Tutorial Video in New Tab"/>
                                <div class="app_settings_block" string="Shipment Quote Tutorial">
                                    <div class="row mt16 o_settings_container" name="qualification_setting_container">
                                        <div controls="true"  class="embed-responsive embed-responsive-16by9">
                                            <iframe width="1080px" height="720px" id="shipment_quote_tutorial_video"  src=""></iframe>
                                        </div>
                                    </div>
                                </div>
                            </page>
