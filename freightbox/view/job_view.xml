<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>


        <!-- ************************ Job ************************** -->
        <!-- ********** Job tree view ********** -->
        <record id="job_tree_view" model="ir.ui.view">
            <field name="name">Job</field>
            <field name="model">job</field>
            <field name="arch" type="xml">
                <tree string="Job" decoration-danger="state == 'hold'"
                      decoration-success="state == 'done'"
                      decoration-bf="state == 'draft'"
                      decoration-primary="state == 'si_created'" 
                      decoration-info="state == 'bol_received'"
                      decoration-muted="state == 'draft_bol'"
                      decoration-warning="carrrier_date &gt; (context_today() + relativedelta(days=-2)).strftime('%Y-%m-%d')"
                      js_class="job_event_dashboard" >
                    <field name="inquiry_id"/>
                    <field name="job_no"/>
                    <field name="job_date"/>
                    <field name="job_state_for_hold" invisible="1"/>
                    <!-- <field name="job_status"/> -->
                    <field name="carrier_booking"/>
                    <field name="carrrier_date"/>
                    <field name="carrrier_id"/>
                    <field name="svc_cont"/>
                    <field name="service_type_origin"/>
                    <field name="service_type_dest"/>
                    <field name="shipment_terms_origin"/>
                    <field name="shipment_terms_dest"/>
                    <field name="state" widget="badge" decoration-danger="state == 'hold'"
                      decoration-success="state == 'done'"
                      decoration-bf="state == 'draft'"
                      decoration-primary="state == 'si_created'" 
                      decoration-info="state == 'bol_received'" decoration-muted="state == 'draft_bol'"/>
                </tree>
            </field>
        </record>
        <record id="view_job_search" model="ir.ui.view">
            <field name="name">job.search</field>
            <field name="model">job</field>
            <field name="arch" type="xml">
                <search string="JOB">
                    <field name="inquiry_id"/>
                    <field name="job_no"/>
                    <field name="carrier_booking"/>
                    <field name="state"/>
                    <separator/>
                        <filter name="job_draft" string="Draft" domain="[('state', '=', 'draft')]"/>
                        <filter name="job_si_accepted" string="SI Accepted" domain="[('state', '=', 'si_accepted')]"/>
                        <filter name="job_draft_bol" string="Draft BoL" domain="[('state', '=', 'draft_bol')]"/>
                        <filter name="job_hold" string="On Hold" domain="[('state', '=', 'hold')]"/>
                        <filter name="job_cargo_released" string="Cargo Released" domain="[('state', '=', 'cargo_released')]"/>
                        <filter name="job_done" string="Done" domain="[('state', '=', 'done')]"/>
                </search>
            </field>
        </record>
        <!-- ********** Job form view ********** -->
        <record id="job_form_view" model="ir.ui.view">
            <field name="name">Job</field>
            <field name="model">job</field>
            <field name="arch" type="xml">
                <form string="Job">
                    <header>
                        <button name="action_job_send" string="Send by Email" type="object" class="btn-primary" states="draft"
                                help="Sends an email to the customer with his login credentials"/>
                        <button name="action_send_link_consignee" string="Send Link To Consignee" type="object" class="btn-primary" states="draft"
                                help="Sends an email to the consignee with his login credentials"/>
                        <button name="action_create_transport" string="Create Transport" type="object"
                                attrs="{'invisible': ['|',('stop_transport_creation', '=', True),('state', 'in', ['hold','inactive'])]}" class="btn-primary"
                                help="Creates Transport for this job,depending on number of confirmed containers"/>
                        <button name="action_create_house_bol" string="Create House BOL" type="object"
                                class="btn-primary" attrs="{'invisible': ['|', '|', ('hbol_count', '&gt;', '0'), ('is_house_bol_needed', '=', False), ('state', 'in', ['hold','inactive'])]}"
                                help="Creates House BOL for this job,if requested by the shipper"/>
                        <!-- <button name="button_done" string="Close Job" states="cargo_released" type="object"
                                class="btn-primary"
                                help="Closes this Job"/> -->
                        <button name="%(freightbox.act_job_hold_wiz)d" states="draft"
                                string="Hold" type="action" class="oe_highlight"
                                attrs="{'invisible': [('state', 'in', ['hold','inactive','done'])]}"/>
                        <!-- <button name="button_hold" string="Hold" type="object" class="btn-primary" attrs="{'invisible': [('state', 'in', ['hold','inactive','done'])]}"/> -->
                        <button name="button_inactive" string="Inactive" type="object" class="btn-primary" invisible="1"/>
                        <!-- <button name="button_settodraft" string="Set to Draft" states='inactive' type="object"
                                class="btn-primary"/> -->
                        <button name="button_release" string="Release" type="object" class="btn-primary" states='hold,inactive'/>
                        <!-- <button name="button_inactive" string="Inactive" type="object" class="btn-primary" attrs="{'invisible': [('state', 'in', ['hold','inactive'])]}"/>
                        <button name="button_settodraft" string="Set to Draft" states='hold,inactive' type="object"
                                class="btn-primary"/> -->
                        <button name="button_invoice" string="Create Invoice" type="object" class="btn-primary" attrs="{'invisible': ['|',('state', 'in', ['hold','inactive','done']),('total_charge', '=', 0)]}"/>
                        <button name="button_container" string="Container Returned" states="cargo_released"
                                confirm="Is all your container returned? "
                                type="object" class="btn-primary"/>
                        <button name="button_close_job" string="Close Job" states="container" type="object"
                                class="btn-primary" help="Closes this Job"/>
                        <button name="button_create_bol" attrs="{'invisible': ['|', ('mbol_count','&gt;', '0'), ('state', 'not in', ('si_accepted'))]}"
                                string="Create Master BOL" type="object" class="btn-primary"/>
                        <button name="import_vessel_location" attrs="{'invisible': [('allow_import_vessel_location','!=', True)]}"
                                string="Import Vessel Location" type="object" class="btn-primary"/>
                        <button
                                name="import_waypoints_from_motherserver"
                                attrs="{'invisible': [('allow_import_way_points','!=', True)]}"
                                string="Import Waypoints"
                                type="object"
                                class="btn-primary"
                        />
                       <!-- <button name="button_receive_bol" string="Receive BOL Motherserver" states="si_accepted" type="object" class="btn-primary"/>
                        <button name="button_send_si" string="Send SI Motherserver" states="si_accepted" type="object" class="btn-primary"/>
                        <button name="button_send_job" string="Send Job Motherserver" type="object" class="btn-primary"/>
                        <button name="button_send_bol" string="Send BOL Motherserver" type="object" class="btn-primary"/>-->
                        <field name="state" widget="statusbar"/>

                    </header>
                    <!-- attrs="{'invisible': [('state', '=', 'draft')]}" -->
                    <sheet>
                        <div class="oe_button_box" name="button_box">
                            <button name="action_get_transport" type="object" class="oe_stat_button"
                                    icon="fa-pencil-square-o" attrs="{'invisible': [('trans_count','&lt;', '1')]}"
                                    help="shows total number of transport for this job">
                                <field name="trans_count" widget="statinfo" string="Transport"/>
                            </button>
                            <button name="action_get_si" type="object" class="oe_stat_button" icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('si_count','&lt;', '1')]}"
                                    help="shows total number of Shipping Instruction for this job">
                                <field name="si_count" widget="statinfo" string="Shipping Instruction"/>
                            </button>
                            <button name="action_get_mbol" type="object" class="oe_stat_button" icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('mbol_count','&lt;', '1')]}"
                                    help="shows number of Master BOL(Bill of Lading) for this job">
                                <field name="mbol_count" widget="statinfo" string="Master BOL"/>
                            </button>
                            <button name="action_get_hbol" type="object" class="oe_stat_button" icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('hbol_count','&lt;', '1')]}"
                                    help="shows number of House BOL(Bill of Lading) for this job">
                                <field name="hbol_count" widget="statinfo" string="House BOL"/>
                            </button>
                            <button name="action_get_invoice"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('invoice_count','&lt;', '1')]}"
                                    help="shows number of Invoices for this job">
                                <field name="invoice_count" widget="statinfo" string="Job Invoice"/>
                            </button>
                            <button name="action_get_mbl_invoice"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('mb_invoice_count','&lt;', '1')]}"
                                    help="shows number of m b/l Invoices for this job">
                                <field name="mb_invoice_count" widget="statinfo" string="M b/l Invoice"/>
                            </button>
                            <button name="action_get_hbl_invoice"
                                    type="object"
                                    class="oe_stat_button"
                                    icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('hb_invoice_count','&lt;', '1')]}"
                                    help="shows number of m b/l Invoices for this job">
                                <field name="hb_invoice_count" widget="statinfo" string="H b/l Invoice"/>
                            </button>
                            <button name="action_get_tnt" type="object" class="oe_stat_button" icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('tnt_count','&lt;', '1')]}"
                                    help="shows if track and trace started for this job">
                                <field name="tnt_count" widget="statinfo" string="Track and Trace"/>
                            </button>
                            <button name="action_get_vessel_location_track" type="object" class="oe_stat_button" icon="fa-pencil-square-o"
                                    attrs="{'invisible': [('vessel_location_track_count','&lt;', '1')]}"
                                    help="shows if vessel location track available for this job">
                                <field name="vessel_location_track_count" widget="statinfo" string="Vessel Track"/>
                            </button>
                        </div>

                        <group colspan="4" col="4">
                            <div class="oe_title">
                                <h1>
                                    <field name="job_no" readonly="1"/>
                                </h1>
                            </div>
                        </group>
                        <separator string="Transport Leg"/>
                        <group>
                            <!--<field name="state" readonly="0"/>-->
                            <group>
                                <field name="vessel_name" required="1" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="voyage" required="1" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="rotation" required="1" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="imo_no" required="1" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                            <group>
                                <field name="load_free_days" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="discharge_free_days" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="detention" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="status" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                        </group>
                        <group colspan="4" col="4">
                            <field name="is_multi_modal" 
                                   attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            <field name="is_house_bol_needed"
                                attrs="{'invisible': [('state', 'in', ['draft','si_created'])], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                        </group>

                        <separator string="Second Vessel Details" attrs="{'invisible': [('is_multi_modal', '=', False)]}"/>
                        <group attrs="{'invisible': [('is_multi_modal', '=', False)]}">
                            <!--<field name="state" readonly="0"/>-->
                            <group>
                                <field name="vessel_name1" attrs="{'invisible': [('is_multi_modal', '=', False)], 'required': [('is_multi_modal', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="voyage1" attrs="{'invisible': [('is_multi_modal', '=', False)], 'required': [('is_multi_modal', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="rotation1" attrs="{'invisible': [('is_multi_modal', '=', False)], 'required': [('is_multi_modal', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                            <group>
                                <field name="imo_no1" attrs="{'invisible': [('is_multi_modal', '=', False)], 'required': [('is_multi_modal', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="intermediate_pol_id" attrs="{'invisible': [('is_multi_modal', '=', False)], 'required': [('is_multi_modal', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="second_vessel_mode" attrs="{'invisible': [('is_multi_modal', '=', False)], 'required': [('is_multi_modal', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="is_another_ship_needed" attrs="{'invisible': [('is_multi_modal', '=', False)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                        </group>

                        <separator string="Third Vessel Details" attrs="{'invisible': [('is_another_ship_needed', '=', False)]}"/>
                        <group attrs="{'invisible': [('is_another_ship_needed', '=', False)]}">
                            <!--<field name="state" readonly="0"/>-->
                            <group>
                                <field name="vessel_name2" attrs="{'invisible': [('is_another_ship_needed', '=', False)], 'required': [('is_another_ship_needed', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="voyage2" attrs="{'invisible': [('is_another_ship_needed', '=', False)], 'required': [('is_another_ship_needed', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="rotation2" attrs="{'invisible': [('is_another_ship_needed', '=', False)], 'required': [('is_another_ship_needed', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                            <group>
                                <field name="imo_no2" attrs="{'invisible': [('is_another_ship_needed', '=', False)], 'required': [('is_another_ship_needed', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="second_intermediate_pol_id" attrs="{'invisible': [('is_another_ship_needed', '=', False)], 'required': [('is_another_ship_needed', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="third_vessel_mode" attrs="{'invisible': [('is_another_ship_needed', '=', False)], 'required': [('is_another_ship_needed', '=', True)], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                        </group>
                        <!-- <group>
                             <field name="is_house_bol_needed"
                                attrs="{'invisible': [('state', 'in', ['draft','si_created'])], 'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                        </group> -->
                        <separator string="Job details"/>
                        <group>
                            <group>
                                <field name="job_no" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="booking_id" required="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                            <group>
                                <field name="job_date" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="job_state_for_hold" invisible="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                        </group>
                        <separator string="Carrier and Service details"/>
                        <group>
                            <!--<field name="booking_id" required="1"/>-->
                            <group>
                                <field name="carrier_booking" required="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="carrrier_date" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="carrrier_id" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="svc_cont" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                            <group>
                                <field name="service_type_origin" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="service_type_dest" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="shipment_terms_origin" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="shipment_terms_dest" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                        </group>
                        <!-- <field name="commodity_hs_code"/> -->
                        <separator string="Cargo details"/>
                        <group>
                            <group>
                                <field name="cargo_gross_weight" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="cargo_uom_id" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                            <group>
                                <field name="commodity_description" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                        </group>
                        <separator string="Date and Container details"/>
                        <group>
                            <group>
                                <field name="requested_date_time" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="actual_date_time" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="requested_equipment_type" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                <field name="confirmed_equipment_type" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                            </group>
                            <group>
                                <field name="exp_no_of_container" readonly="1"/>
                                <field name="confirmed_no_of_container" required="1"/>
                                <field name="reference_type" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
    
                                <field name="transport_ids" widget="many2many_tags" invisible="1"/>
                                <!-- <field name="reference_name"/> -->
                                <!-- <field name="reference_description"/> -->
                                <!--<field name="shipping_instruction" colspan="4" invisible="1"/>-->
                                <!-- <field name="si_template_ids" force_save="1"  widget="many2many_tags"/>-->
                                <field name="shipping_instruction_ids" force_save="1" invisible="1" domain="[('carrier_booking_reference', '=', carrier_booking)]" widget="many2many_tags"/>
                                <field name="shipping_description" colspan="4" invisible="1"/>
                                <field name="shipment_id" invisible="1"/>
                                <field name="transport_count" invisible="1"/>
                                <field name="stop_transport_creation" invisible="1"/>
                                <field name="requested_equip_unit_id" invisible="1"/>
                                <field name="confirmed_equip_unit_id" invisible="1"/>
                                <field name="reference_id" invisible="1"/>
                                <field name="is_send_mail" invisible="1"/>
                                <field name="is_cargo_send_mail" invisible="1"/>
                            </group>
                        </group>
                        <notebook>
                            <page name='Other_information' string="Other Info">
                                <group>
                                    <group>
                                        <field name="so_id" string="Sale order" readonly="1" force_save="1" context="{'form_view_ref': 'freightbox.sale_order_form_view_freightbox'}"/>
                                        <field name="po_id" string="Purchase order" readonly="1" force_save="1" context="{'form_view_ref': 'freightbox.purchase_order_form'}"/>
                                        <field name="shipper_id" string="Shipper" readonly="1" force_save="1"/>
                                        <field name="inquiry_id" string="Inquiry" options='{"no_open": False, "no_create": True}' context="{'form_view_ref': 'freightbox.crm_lead_view_form_freightbox'}"/>
                                        <field name="shipping_instruction_ids" readonly="1" widget="many2many_tags" string="Open SI IDs" force_save="1" invisible="0" />
                                        <field name="user_id" string="User" readonly="0" invisible="0" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                        <field name="confirmed_transport_ids" widget="many2many_tags" readonly="1" force_save="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                        <field name="is_cont_released" force_save="1"/>
                                    </group>
                                    <group>
                                        <field name="place_of_origin" required="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                        <field name="final_port_of_destination" required="1" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                        <field name="port_of_origin_id" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                        <field name="final_port_of_destination_id" attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                        <field name="active" invisible="1"/>
                                        <field name="booking_user_id" readonly="1"/>
                                        <field name="hold_bool" invisible="1"/>
                                        <field name="lay_time"/>
                                    </group>
                                </group>
                            </page>
                            <page name="cargo_plus" string="Cargo Plus">
                                <separator string="Additional Cargo Details"/>
                                <field name="cargo_plus_ids"/>
                            </page>
                            <page string="Charges">
                                <field name="charge_line"
                                       attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}">
                                    <tree string="charges" editable="bottom">
                                        <field name="charge_type"/>
                                        <field name="currency_code"/>
                                        <field name="payment_term"/>
                                        <field name="calculation_basis"/>
                                        <field name="quantity"/>
                                        <field name="unit_price"/>
                                        <field name="currency_amount"/>
                                        <!--<field name="final_amount" readonly="1"/>-->
                                    </tree>
                                </field>
                                <group class="oe_subtotal_footer oe_right" colspan="2" name="sale_total">
                                    <div>
                                        <label for="total_charge"/>
                                    </div>
                                    <field name="total_charge" nolabel="1" widget='monetary'
                                           options="{'currency_field': 'currency_id'}"/>
                                </group>
                            </page>
                            <page string="Agents">
                                <field name="agents_line"
                                       attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}">
                                    <tree string="Agents" editable="bottom">
                                        <field name="agent_type"/>
                                        <field name="agent_name" domain="[('contact_type','=','agent')]"/>
                                        <field name="agent_loc"/>
                                        <field name="charge"/>
                                        <!--<field name="final_amount" readonly="1"/>-->
                                    </tree>
                                </field>
                            </page>
                            <page name='Accounting' string="Accounting">
                                <group>
                                    <field name="invoice_ids" widget="many2many_tags"
                                        attrs="{'readonly': ['|',('state', '=', 'hold'),('state', '=', 'inactive')]}"/>
                                    <field name="invoice_status"/>
                                </group>
                            </page>
                            <page name="Hold Reason" string="Hold Reason"
                                  attrs="{'invisible': [('hold_bool','!=', True)]}">
                                <group colspan="2" col="2">
