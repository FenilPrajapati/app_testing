    <!--Footer-->
    <template id="template_footer_custom" inherit_id="website.footer_custom" name="Default" active="True">
        <xpath expr="//div[@id='footer']" position="replace">
            <div id="footer">
                <footer >
                    <!-- <div class="p-md-5 p-lg-5 mb-xxl-5">&nbsp;</div> -->
                    <div class="wave"></div>
                    <div class="container py-3 py-md-5  text-light">
                        <div class="row footer-contain top-section g-2 mt-5">

                            <div class="column enquiry_block col-md-12 col-lg-5">
                                <h3 class="mb-5">Enquiry</h3>
                                <p>Email us with any questions or inquiries or use our contact data. </p>
                                <form action="" class="row g-3">
                                    <div class="col-md-6">
                                        <input type="text" class="form-control" placeholder="First Name"/>
                                    </div>

                                    <div class="col-md-6">
                                        <input type="text" class="form-control" placeholder="Last Name"/>
                                    </div>

                                    <div class="col-md-12">
                                        <input type="email" class="form-control" placeholder="Email Address"/>
                                    </div>

                                    <div class="col-md-12">
                                        <textarea class="form-control" rows="3" placeholder="Message"></textarea>
                                    </div>

                                    <div class="col-md-12">
                                        <button type="submit" class="btn btn-primary border-0">Sign in</button>
                                    </div>
                                </form>
                            </div>

                            <div class="column sitemap_block col-md-4 col-lg-2">
                                <h3 class="mb-5">Site Map</h3>

                                <ul class="nav d-flex flex-column">
                                    <li class="nav-list"><a href="#" class="btn btn-link text-white">About Us</a></li>
                                    <li class="nav-list"><a href="#" class="btn btn-link text-white">Why Us?</a></li>
                                    <li class="nav-list"><a href="#" class="btn btn-link text-white">Features</a></li>
                                    <li class="nav-list"><a href="#" class="btn btn-link text-white">Contact Us</a></li>
                                    <li class="nav-list"><a href="#" class="btn btn-link text-white">Blog</a></li>
                                </ul>
                            </div>

                            <div class="column contact_block col-md-8 col-lg-5">
                                <h3 class="mb-5">Contact Us</h3>

                                <ul class="nav d-flex flex-column contact-data">
                                    <li>
                                        <i class="fa-solid fa-location-dot"></i>
                                        <p class="contact-contain ps-5">
                                            <b>Powerp Box IT Solutions Pvt Ltd,</b><br/>
                                            216, Bestech Chambers, Sushant Lok1, Sector 27 Gurgaon, Haryana 122002 India
                                        </p>
                                    </li>
                                    <li>
                                        <i class="fa-solid fa-envelope"></i>
                                        <p class="contact-contain ps-5">
                                            Email: info@powerpbox.org
                                        </p>
                                    </li>
                                    <li>
                                        <i class="fa-solid fa-phone-flip"></i>
                                        <p class="contact-contain ps-5">
                                        Tel No: +91 7506926394
                                        </p>
                                    </li>
                                </ul>
                            </div>

                        </div>

                        <div class=" bottom-section py-3 mt-5 border-top">
                        <p class="text-center ">Copyright 2021 All rights reserved.</p>
                        </div>
                    </div>
                </footer>
            </div>
        </xpath>
    </template>

</odoo>