        <record id="search_inherit_res_partner" model="ir.ui.view">
            <field name="name">res.partner.view.search.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_res_partner_filter"/>
            <field name="arch" type="xml">
                <xpath expr="//filter[@name='inactive']" position="before">
                    <filter string="Shippers" name="filter_shippers" domain="[('contact_type', '=', 'shippers')]"/>
                    <filter string="Consignees" name="filter_consignees" domain="[('contact_type', '=', 'consignees')]"/>
                    <filter string="Freight Forwarders" name="filter_freightforwarders" domain="[('contact_type', '=', 'freightforwarders')]"/>
                    <filter string="NVOCC" name="filter_NVOCC" domain="[('contact_type', '=', 'NVOCC')]"/>
                    <filter string="Vendors Shipping Lines" name="filter_vendors_shipping" domain="[('contact_type', '=', 'vendors_shipping_lines')]"/>
                    <filter string="agent" name="filter_agent" domain="[('contact_type', '=', 'agent')]"/>
                    <separator/>
                    <!-- <filter string="BlackList" name="filter_blacklist" domain="[('is_blacklist', '=', True)]"/>
                    <filter string="Active" name="filter_active" domain="[('is_cust_active', '=', True)]"/>
                    <filter string="In Active" name="filter_in_active" domain="[('is_cust_active', '!=', True)]"/> -->

                </xpath>
            </field>
        </record>

