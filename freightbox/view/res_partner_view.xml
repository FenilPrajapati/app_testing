    <data>
        <!-- Show company name on tree. If contact, show contact name. if company, show company name - by default it shows both -->
        <record model="ir.ui.view" id="view_partner_tree_for_enquiry">
            <field name="name">res.partner.tree.form.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="attributes">
                    <attribute name="js_class">partner_event_dashboard</attribute>
                    <!-- <attribute name="decoration-success">is_cust_active == True</attribute>
                    <attribute name="decoration-danger">is_cust_active == False</attribute>
                    <attribute name="decoration-warning">is_approach_inactive == True</attribute> -->
                </xpath>
                <xpath expr="/tree/field[@name='company_id']" position="replace">
                    <field name="company_id" optional="hide" readonly="1"/>
                    <!-- <field name="is_cust_active" invisible="1"/>
                    <field name="is_approach_inactive" invisible="1"/> -->
                </xpath>
                <xpath expr="/tree/field[@name='email']" position="after">
                    <field name="customer_contact_name" string="Company Name"/>
                </xpath>
            </field>
        </record>

